<column index="3" value="49"/><column index="4" value="65"/><column index="5" value="118"/><column index="6" value="70"/><column index="7" value="49"/><column index="8" value="69"/><column index="9" value="61"/><column index="10" value="59"/><column index="11" value="54"/><column index="12" value="47"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1*">UPDATE clasificacion 
SET puntaje_total = (
    (SELECT SUM(goles) FROM partido WHERE id_equipo = clasificacion.id_equipo) * 0.35 +
    (SELECT SUM(posesion) FROM partido WHERE id_equipo = clasificacion.id_equipo) * 0.2 +
    (SELECT SUM(remates) FROM partido WHERE id_equipo = clasificacion.id_equipo) * 0.2 +
    (SELECT SUM(remates_directos) FROM partido WHERE id_equipo = clasificacion.id_equipo) * 0.2 -
    (SELECT SUM(faltas) FROM partido WHERE id_equipo = clasificacion.id_equipo) * 0.15
) 
WHERE id_equipo IN (SELECT DISTINCT id_equipo FROM partido);</sql><current_tab id="0"/></tab_sql></sqlb_project>
